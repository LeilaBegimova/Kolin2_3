    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.kolin2_3.presentation.ui.fragments.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/viewPagerFragment"
        android:name="com.example.kolin2_3.presentation.ui.fragments.onbord.ViewPagerFragment"
        android:label="fragment_on_bord_paging_first"
        tools:layout="@layout/fragment_on_bord_paging_first" >
        <action
            android:id="@+id/action_viewPagerFragment_to_homeFragment"
